# Ambiente
NODE_ENV=production
PORT=3333

# Database (PostgreSQL do Metta Studio)
DATABASE_URL=postgresql://metta_user:04022002Gui%40@metta_postgres:5432/metta_db?schema=public&client_encoding=utf8&application_name=zapflow

# Redis (próprio do ZapFlow)
REDIS_URL=redis://redis:6379

# Metta Studio API (Backend Principal)
METTA_API_BASE_URL=http://metta_backend:3001
METTA_API_TIMEOUT=10000
METTA_USERNAME=admin
METTA_PASSWORD=SUA_SENHA_AQUI

# Token refresh (opcional, em milissegundos - 50min)
